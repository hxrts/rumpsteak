(*# RefinementTypes #*)

global protocol RingMax(role A, role B, role C, role D, role E, role F, role G)
{
	ProposalA(a: int) from A to B;
	ProposalB(b: int) from B to C;
	ProposalC(c: int) from C to D;
	ProposalD(d: int) from D to E;
	ProposalE(e: int) from E to F;
	ProposalF(f: int) from F to G;
	ProposalG(g: int) from G to A;
}
